<p id="notice"><%= notice %></p>

<h1>Listing Parry Scores</h1>

<table>
  <thead>
    <tr>
      <th>Weapon</th>
      <th>Parry</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parry_scores.each do |parry_score| %>
      <tr>
        <td><%= parry_score.weapon %></td>
        <td><%= parry_score.parry %></td>
        <td><%= link_to 'Show', parry_score %></td>
        <td><%= link_to 'Edit', edit_parry_score_path(parry_score) %></td>
        <td><%= link_to 'Destroy', parry_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parry score', new_parry_score_path %>
