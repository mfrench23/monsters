<p id="notice"><%= notice %></p>

<h1>Listing Equipment Modifier Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% equipment_modifier_categories.each do |equipment_modifier_category| %>
      <tr>
        <td><%= equipment_modifier_category.name %></td>
        <td><%= equipment_modifier_category.notes %></td>
        <td><%= link_to 'Show', equipment_modifier_category %></td>
        <td><%= link_to 'Edit', edit_equipment_modifier_category_path(equipment_modifier_category) %></td>
        <td><%= link_to 'Destroy', equipment_modifier_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Equipment modifier category', new_equipment_modifier_category_path %>
