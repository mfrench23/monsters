<p id="notice"><%= notice %></p>

<h1>Listing Page References</h1>

<table>
  <thead>
    <tr>
      <th>Book</th>
      <th>Monster</th>
      <th>Pages</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @page_references.each do |page_reference| %>
      <tr>
        <td><%= page_reference.book %></td>
        <td><%= page_reference.monster %></td>
        <td><%= page_reference.pages %></td>
        <td><%= link_to 'Show', page_reference %></td>
        <td><%= link_to 'Edit', edit_page_reference_path(page_reference) %></td>
        <td><%= link_to 'Destroy', page_reference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page reference', new_page_reference_path %>
