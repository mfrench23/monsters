<% link_data_attributes ||= {} %>
<%# ------------------- %>
<% links = list.map{ |item| {:name => item.name, :url => url_for(item), :opts => link_data_attributes.merge(:title => item.name)} } %>
<% count_header = pluralize(links.count, "item") %>
<%# ------------------- %>
<%= react_component 'BoxedList', {countHeader: count_header, links: links}, { tag: "span" } %>

